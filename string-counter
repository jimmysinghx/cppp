#include <iostream>
#include <unordered_map>
#include <string>
using namespace std;

int main(){
    unordered_map<char,int> countMap;
    const string fruit = "banana" ;
    
    for(int i = 0  ; i<fruit.size() ; i++){
        if(countMap.find(fruit[i])==countMap.end()){
            countMap.insert({fruit[i] , 1});
        }
        else{
            countMap[fruit[i]]++ ;
        }
    }
    for(const auto&p : countMap) {
        cout<<p.first<<" "<<p.second<<endl;
    }
}
